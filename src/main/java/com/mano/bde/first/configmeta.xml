<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="
          http://www.springframework.org/schema/beans
          
          http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Bean Definitions go here -->
    <bean name="cus" class="com.mano.bde.first.Customers">
    <property name="customerName">
    	<value>manoj</value>
    </property>
    
    <property name="customerId">
    	<value>0900</value>
    </property>
    
    <property name="customerAddress">
    	<value>isudish</value>
    </property>
    
    <property name="orderId">
    	<value>0900</value>
    </property>
    </bean>
    
    <!--  bean properties of Order class-->
    <bean name="ord" class="com.mano.bde.first.Orders" >
    <property name="Date" value="2012-12-20"></property>
    <property name="productId" value="9080"></property>
    <property name="productName" value="varnamajhjd"></property>
    </bean>
    
    <bean name="cus2" class="com.mano.bde.first.Customers"
    
    p:customerName="nani" p:customerId="890i" p:orderId="khfdfjks"
    
